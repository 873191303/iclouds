<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.h3c.iclouds.po.HostGroup" table="ipm_pft_groups" >
        <id name="hostGroupId">
            <column name="groupid" sql-type="bigint"/>
            <generator class="assigned"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="displayName">
            <column name="displayname" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="internal">
            <column name="internal" sql-type="integer"/>
        </property>
        <property name="flags">
            <column name="flags" sql-type="integer"/>
        </property>
        <property name="tenantId">
            <column name="tenantid" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="createdBy" update="false">
            <column name="createdby" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="createdDate" update="false">
            <column name="createddate" sql-type="timestamp"/>
        </property>
        <property name="updatedBy">
            <column name="updatedby" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="updatedDate">
            <column name="updateddate" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>