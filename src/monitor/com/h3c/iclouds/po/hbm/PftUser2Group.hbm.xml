<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.h3c.iclouds.po.PftUser2Group" table="ipm_pft_users2groups" >
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="usrGrpId">
            <column name="usrgrpid" sql-type="bigint"/>
        </property>
        <property name="userId">
            <column name="userid" sql-type="bigint"/>
        </property>
        <property name="createdBy" update="false">
            <column name="createdby" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="createdDate" update="false">
            <column name="createddate" sql-type="timestamp"/>
        </property>
        <property name="updatedBy">
            <column name="updatedby" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="updatedDate">
            <column name="updateddate" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>