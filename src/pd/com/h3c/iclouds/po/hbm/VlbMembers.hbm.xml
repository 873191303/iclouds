<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.h3c.iclouds.po.VlbMember" table="iyun_vlb_members">
		<id name="id" type="string">
			<column name="id" length="36"/>
			<generator class="uuid.hex"/>
		</id>
		<property name="status" type="string">
			<column name="status" length="16" />
		</property>
		<property name="statusDescription" type="string">
			<column name="status_descrition" length="255" />
		</property>
		<property name="poolId" type="string" update="false">
			<column name="pool_id" length="36" />
		</property>
		<property name="address" type="string">
			<column name="address" length="64" />
		</property>
		<property name="protocolPort" type="int">
			<column name="protocol_port" length="4"/>
		</property>
		<property name="weight" type="int">
			<column name="weight" length="4"/>
		</property>
		<property name="adminStateUp" type="boolean">
			<column name="admin_state_up" />
		</property>
		<property name="tenantId" type="string" update="false">
			<column name="tenant_id" length="50" />
		</property>
		<property name="cloudosId" type="string">
			<column name="cloudos_id" length="50" />
		</property>
		<property name="vmId" type="string" update="false">
			<column name="vm_id" length="50" />
		</property>
		<property name="createdBy" type="string" update="false">
			<column name="CreatedBy" length="36" />
		</property>
		<property name="createdDate" type="timestamp" update="false">
			<column name="CreatedDate" length="19" />
		</property>
		<property name="updatedBy" type="string">
			<column name="UpdatedBy" length="36" />
		</property>
		<property name="updatedDate" type="timestamp">
			<column name="UpdatedDate" length="19" />
		</property>
		<property name="poolCloudosId" type="string" formula="(SELECT pool.cloudos_id FROM iyun_vlb_pools pool WHERE pool.id = pool_id)"/>
		<property name="vmName" type="string" formula="(SELECT vm.hostName FROM iyun_nova_vm vm WHERE vm.id = vm_id)"/>
	</class>
</hibernate-mapping>
