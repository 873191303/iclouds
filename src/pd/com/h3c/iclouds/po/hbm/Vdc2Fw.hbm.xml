<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.h3c.iclouds.po.Vdc2Fw" table="iyun_vdc_vdc2fw">
        <id name="id">
            <column name="id" sql-type="varchar(32)" length="32"/>
            <generator class="uuid.hex"/>
        </id>
        <property name="vdcId">
            <column name="vdcid" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="firewallId">
            <column name="fireware" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="throughPut">
            <column name="throughput" sql-type="bigint"/>
        </property>
        <property name="sequence">
            <column name="sequence" sql-type="smallint"/>
        </property>
        <property name="createdBy">
            <column name="createdby" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="createdDate">
            <column name="createddate" sql-type="timestamp"/>
        </property>
        <property name="updatedBy">
            <column name="updatedby" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="updatedDate">
            <column name="updateddate" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>