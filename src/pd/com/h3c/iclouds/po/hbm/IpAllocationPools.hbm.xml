<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.h3c.iclouds.po.IpAllocationPool" table="iyun_subnet_ipallocationpools">
		<id name="id" type="string">
			<column name="id" length="36"/>
			<generator class="uuid.hex"/>
		</id>
		<property name="subnetId" type="string">
			<column name="subnet_id" length="36" />
		</property>
		<property name="firstIp" type="string">
			<column name="first_ip" length="64" />
		</property>
		<property name="lastIp" type="string">
			<column name="last_ip" length="64" />
		</property>
	</class>
</hibernate-mapping>
