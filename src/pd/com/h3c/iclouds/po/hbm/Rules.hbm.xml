<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.h3c.iclouds.po.Rules" table="iyun_base_images">
        <id name="id" type="string">
            <column name="id" length="36" />
            <generator class="assigned" />
        </id>
        <property name="osMirId" type="string">
            <column name="osMirId" length="36"/>
        </property>
        <property name="osMirName" type="string">
            <column name="osMirName" length="100"/>
        </property>
        <property name="vcpu" type="integer">
            <column name="vcpu"/>
        </property>
        <property name="minDisk" type="integer">
            <column name="minDisk" />
        </property>
        <property name="minRam" type="integer">
            <column name="minRam" />
        </property>
        <property name="minSwap" type="integer">
            <column name="minSwap" />
        </property>
        <property name="isDefault" type="string">
            <column name="isDefault" length="1" />
        </property>
        <property name="syncTime" type="timestamp">
            <column name="syncTime" length="19"/>
        </property>
        <property name="format" type="string">
            <column name="format" length="100"/>
        </property>
        <property name="osType" type="string">
            <column name="ostype" length="100"/>
        </property>
        <property name="tenantId" type="string">
            <column name="tenantId" length="50"/>
        </property>
        <property name="userId" type="string">
            <column name="userId" length="50"/>
        </property>
        <property name="definition" type="string">
            <column name="definition" length="36"/>
        </property>
         <property name="createdBy" type="string" update="false">
            <column name="CreatedBy" length="36" />
        </property>
        <property name="createdDate" type="timestamp" update="false">
            <column name="CreatedDate" length="19"/>
        </property>
        <property name="updatedBy" type="string">
            <column name="UpdatedBy" length="36" />
        </property>
        <property name="updatedDate" type="timestamp">
            <column name="UpdatedDate" length="19" />
        </property>
    </class>
</hibernate-mapping>