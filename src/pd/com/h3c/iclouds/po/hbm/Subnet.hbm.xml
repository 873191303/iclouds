<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.h3c.iclouds.po.Subnet" table="iyun_vdc_subnet">
		<id name="id" type="string">
			<column name="id" length="36" />
			<generator class="uuid.hex" />
		</id>
		<property name="name" type="string">
			<column name="name" length="255" />
		</property>
		<property name="networkId" type="string" update="false">
			<column name="network_id" length="36" />
		</property>
		<property name="ipVersion" type="int">
			<column name="ip_version" length="4" />
		</property>
		<property name="cidr" type="string">
			<column name="cidr" length="64" />
		</property>
		<property name="gatewayIp" type="string">
			<column name="gateway_ip" length="64" />
		</property>
		<property name="enableDhcp" type="boolean">
			<column name="enable_dhcp" />
		</property>
		<property name="shared" type="boolean">
			<column name="shared" />
		</property>
		<property name="ipv6RaMode" type="string">
			<column name="ipv6_ra_mode" length="50" />
		</property>
		<property name="ipv6AddressMode" type="string">
			<column name="ipv6_address_mode" length="50" />
		</property>
		<property name="subnetPoolId" type="string">
			<column name="subnetPool_id" length="50" />
		</property>
		<property name="tenantId" type="string" update="false">
			<column name="tenant_id" length="255" />
		</property>
		<property name="cloudosId" type="string">
			<column name="cloudos_id" length="50" />
		</property>
		<property name="createdBy" type="string" update="false">
			<column name="CreatedBy" length="36" />
		</property>
		<property name="createdDate" type="timestamp" update="false">
			<column name="CreatedDate" length="19" />
		</property>
		<property name="updatedBy" type="string">
			<column name="UpdatedBy" length="36" />
		</property>
		<property name="updatedDate" type="timestamp">
			<column name="UpdatedDate" length="19" />
		</property>
		<set name="ipUseds" table="iyun_vdc_ipallocations" lazy="false" inverse="true">
			<key column="subnet_id"></key>
			<one-to-many class="com.h3c.iclouds.po.IpAllocation"/>
		</set>
	</class>
</hibernate-mapping>
