<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.h3c.iclouds.po.TaskDispatch" table="auto_sync_taskdispatch">
        <id name="id" type="string">
            <column name="id" length="36" />
            <generator class="uuid.hex" />
        </id>
        <property name="dbKey" type="string">
            <column name="dbKey" length="50"/>
        </property>
        <property name="syncType" type="string">
            <column name="syncType" length="50"/>
        </property>
        <property name="startTime" type="timestamp">
            <column name="startTime" />
        </property>
        <property name="startDate" type="timestamp">
            <column name="startDate"/>
        </property>
        <property name="endType" type="string">
            <column name="endType" length="50"/>
        </property>
        <property name="endDate" type="timestamp">
            <column name="endDate" />
        </property>
        <property name="endTimes" type="integer">
            <column name="endTimes" />
        </property>
        <property name="endOverTimes" type="integer">
            <column name="endOverTimes" />
        </property>
        <property name="safeType" type="string">
            <column name="safeType" length="50"/>
        </property>
        <property name="safeTimes" type="integer">
            <column name="safeTimes" />
        </property>
        <property name="periodType" type="integer">
            <column name="periodType" />
        </property>
        <property name="dayInterval" type="integer">
            <column name="dayInterval" />
        </property>
        <property name="weekInterval" type="string">
            <column name="weekInterval" length="50"/>
        </property>
        <property name="monthInterval" type="integer">
            <column name="monthInterval" />
        </property>
        <property name="monthType" type="string">
            <column name="monthType" length="50"/>
        </property>
        <property name="monthDay" type="string">
            <column name="monthDay" length="50"/>
        </property>
        <property name="yearInterval" type="integer">
            <column name="yearInterval" />
        </property>
        <property name="yearType" type="string">
            <column name="yearType" length="50"/>
        </property>
        <property name="yearDay" type="string">
            <column name="yearDay" length="50"/>
        </property>
        <property name="nextSyncTime" type="timestamp">
            <column name="nextSyncTime" />
        </property>
        <property name="lastSyncTime" type="timestamp">
            <column name="lastSyncTime" />
        </property>
        <property name="remark" type="string">
            <column name="remark" length="50"/>
        </property>
        <property name="mail" type="string">
            <column name="mail" length="50"/>
        </property>
        <property name="groupId" type="string">
            <column name="groupId" length="36" />
        </property>
        <property name="createdBy" type="string" update="false">
            <column name="CreatedBy" length="36" />
        </property>
        <property name="createdDate" type="timestamp" update="false">
            <column name="CreatedDate" length="19"/>
        </property>
        <property name="updatedBy" type="string">
            <column name="UpdatedBy" length="36" />
        </property>
        <property name="updatedDate" type="timestamp">
            <column name="UpdatedDate" length="19" />
        </property>
    </class>
</hibernate-mapping>
