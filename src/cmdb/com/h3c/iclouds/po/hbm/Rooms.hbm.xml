<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.h3c.iclouds.po.Rooms" table="cmdb_dc_rooms">
		<id name="id" type="string">
			<column name="id" length="36" />
			<generator class="uuid.hex" />
		</id>
		<property name="roomName" type="string">
			<column name="roomName" length="100" />
		</property>
		<property name="shortName" type="string">
			<column name="shortName" length="100" />
		</property>
		<property name="maxRows" type="integer">
			<column name="maxRows" />
		</property>
		<property name="maxCols" type="integer">
			<column name="maxCols" />
		</property>
		<property name="defaultU" type="integer">
			<column name="defaultU" />
		</property>
		<property name="capacity" type="integer">
			<column name="capacity" />
		</property>
		<property name="telephone" type="string">
			<column name="telephone" length="20" />
		</property>
		<property name="roomOnwer" type="string">
			<column name="roomOnwer" length="50" />
		</property>
		<property name="localAdmin" type="string">
			<column name="localAdmin" length="50" />
		</property>
		<property name="localAdminTel" type="string">
			<column name="localAdminTel" length="50" />
		</property>
		<property name="admin" type="string">
			<column name="admin" length="50" />
		</property>
		<property name="contact" type="string">
			<column name="contact" length="100" />
		</property>
		<property name="region" type="string">
			<column name="region" length="50" />
		</property>
		<property name="address" type="string">
			<column name="address" length="100" />
		</property>
		<property name="adminTel" type="string">
			<column name="adminTel" length="20" />
		</property>
		<property name="supplier" type="string">
			<column name="supplier" length="100" />
		</property>
		<property name="remark" type="string">
			<column name="remark" length="100" />
		</property>
		<property name="groupId" type="string">
			<column name="groupId" length="36" />
		</property>
		<property name="createdBy" type="string" update="false">
			<column name="CreatedBy" length="36" />
		</property>
		<property name="createdDate" type="timestamp" update="false">
			<column name="CreatedDate" length="19" />
		</property>
		<property name="updatedBy" type="string">
			<column name="UpdatedBy" length="36" />
		</property>
		<property name="updatedDate" type="timestamp">
			<column name="UpdatedDate" length="19" />
		</property>

	</class>
</hibernate-mapping>
