<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.h3c.iclouds.po.business.EventType" table="iyun_measure_eventtype" >
        <id name="id">
            <column name="id" sql-type="varchar(36)" length="36"/>
            <generator class="uuid.hex"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="classId">
            <column name="classid" sql-type="varchar(36)" length="36"/>
        </property>
    </class>
</hibernate-mapping>